    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "tailwindcss": "4.0.0-beta.3",
    "typescript": "~5.6.2",
    "vite": "^7.0.1",
    "vite-bundle-analyzer": "^0.11.0",
    "vite-plugin-pwa": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
